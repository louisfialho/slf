{
  "name": "Shelf",
  "version": "1.0",
  "description": "Save resources from the web to your Shelf.",
  "manifest_version": 2,
  "permissions": [ "activeTab", "cookies", "http://*/*", "https://*/*", "https://www.shelf.so/"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+I"
      }
    }
  },
  "icons": { "48": "logo.png" }
}





