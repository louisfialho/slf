      <p>user username : <%= @user.username %></p>
      <p>user email : <%= @user.email %></p>
      <p>user phone_number : <%= @user.phone_number %></p>
      <p>user telegram_hash : <%= @user.telegram_hash %></p>
    </div>
  </body>
